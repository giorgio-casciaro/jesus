{
  "name": "jesus",
  "version": "1.0.0",
  "description": "Jesus Packages",
  "author": "Giorgio Casciaro <giorgio.casciaro@gmail.com>",
  "main": "jesus.js",
  "scripts": {
    "start": "npm --silent run-script wach_test",
    "wach_test_single": "nodemon --ext js ./test/tests.js",
    "pm2_wach_test": "pm2 start pm2.js --only worker-app ",
    "wach_test": "nodemon  --ext js --exec \"clear && printf '\\e[3J' && npm --silent test || true\"",
    "test": "tap *.test.js */*.test.js  --color --reporter=spec",
    "test_coverage": "tap *.test.js --color --cov --reporter=spec --coverage-report=html",
    "debug": "node-debug jesus.test.js"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-regenerator": "^6.21.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2017": "^6.16.0",
    "nodemon": "^1.11.0",
    "restler": "^3.4.0",
    "tap": "^7.1.2",
    "tingodb": "^0.5.1"
  },
  "dependencies": {
    "ajv": "^4.10.3",
    "body-parser": "^1.16.0",
    "express-http2": "^4.13.4",
    "grpc": "^1.1.1",
    "helmet": "^3.4.0",
    "mongodb": "^2.2.16",
    "ramda": "^0.23.0",
    "sift": "^3.2.6",
    "source-map-support": "^0.4.10",
    "stack-trace": "0.0.9",
    "uuid": "^3.0.1",
    "winston": "^2.3.1"
  }
}
